<!-- /sections/flexi-product.liquid -->
<!-- Brandon Landgraff - Flexi Studio -->
{{ 'flexi-styling.css' | asset_url | stylesheet_tag }}
{{ 'flexi-javascript.js' | asset_url | script_tag }}

{%- liquid
    assign sectionid = section.id

    assign product = section.settings.product
    assign selected_variant = product.selected_or_first_available_variant
    assign product_image = product.images[0].src
    assign image = section.settings.image
    assign unique_id = 'flexi-' | append: section.id
    assign form_id = unique_id | append: '-product-form'

    assign bg_color = section.settings.bg_color
    assign width = section.settings.width
    assign grid_layout = section.settings.grid_layout

    assign padding_top = section.settings.padding_top
    assign padding_bottom = section.settings.padding_bottom
    assign mobile_top_padding = section.settings.padding_top | times: 0.6 | round
    assign mobile_bottom_padding = section.settings.padding_bottom | times: 0.6 | round

    assign mb_reverse_order = section.settings.mb_reverse_order
    assign dt_reverse_order = section.settings.dt_reverse_order

    assign json_data = shop.metafields.flexi.flexistyling.value
    assign borderRadius = json_data.borderRadius
    assign animation = json_data.animation
  
    if json_data == blank
        assign borderRadius = 8
        assign animation = true
    endif

    if section.index <= 3
      assign limit_animation = false
    else
      assign limit_animation = true
    endif

    if section.index <= 3
      assign loading_strategy = 'eager'
      assign fetch_priority = 'high'
    else
      assign loading_strategy = 'lazy'
      assign fetch_priority = 'auto'
    endif
  
    assign widths = '165, 360, 535, 750, 1070, 1500'
  
  
    assign img_alt = image.alt | escape
  -%}
  {%- capture sizes -%}
    (min-width: 1200px) calc((1200px - 100px) / 2),
    (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)
  {%- endcapture -%}
  
{%- style -%}
.flexi-product--{{ sectionid }} {
    background-color: {{ bg_color }};
    padding-top: {{ padding_top }}px;
    padding-bottom: {{ padding_bottom }}px;
    padding-right: 2em;
    padding-left: 2em;
}

@media only screen and (max-width: 768px) {
    .flexi-product--{{ sectionid }} {
        padding-top: {{ mobile_top_padding }}px;
        padding-bottom: {{ mobile_bottom_padding }}px;
        padding-right: 0.5em;
        padding-left: 0.5em;∆í
    }
}

.flexi-blocks-wrapper--{{ sectionid }} {
    max-width: {{ width }}px;
}

.flexi-blocks--{{ sectionid }} {
    display: grid;
    justify-content: center;
    gap: 12px;
}
  
@media only screen and (min-width: 768px) {
    .flexi-blocks-wrapper--{{ sectionid }} {
        display: flex;
        justify-content: center;
        align-items: center;
        margin: auto;
    }

    .flexi-blocks--{{ sectionid }} {   
        grid-template-columns: {{ grid_layout }};
        height: 100%;
        width: 100%;
        gap: 24px;
    }
}

#flexi-col-image-{{ sectionid }} {
    display: block;
    width: 100%;
    height: fit-content;
    align-self: center;
}
#flexi-col-image-{{ sectionid }} img {
    display: block;
    width: 100%;
    height: auto;
    border-radius: {{ borderRadius }}px;
}

#flexi-col-image-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    border-radius: {{ borderRadius }}px;
    background-color: #D4D4D4;
}

@media only screen and ( min-width: 769px ) {  
    {% if dt_reverse_order == 'left' %}
       .flexi-blocks--{{ sectionid }} .flexi-col-{{ sectionid }}:nth-child(1) {
          order: 2;
        }
    {% elsif dt_reverse_order == 'right' %}
        .flexi-blocks--{{ sectionid }} .flexi-col-{{ sectionid }}:nth-child(2) {
          order: 1;
        }
    {% endif %}
}
@media only screen and ( max-width: 768px ) {  
    {% if mb_reverse_order == 'top' %}
        .flexi-blocks--{{ sectionid }} .flexi-col-{{ sectionid }}:nth-child(1) {
          order: 2;
        }
    {% elsif mb_reverse_order == 'bottom' %}
        .flexi-blocks--{{ sectionid }} .flexi-col-{{ sectionid }}:nth-child(2) {
          order: 1;
        }
    {% endif %}

    .flexi-blocks--{{ sectionid }} {
      grid-template-columns: 1fr;
    }
}

.flexi-blocks-container--{{ sectionid }} {
      align-self: center;
      padding: 4em;
      margin: auto;
  }
  @media only screen and ( max-width: 768px ) {
      .flexi-blocks-container--{{ sectionid }} {
          padding: 0px 1em;
          margin: 0px;
      }
  }
{%- endstyle -%}
  
